{
  "name": "code-base-react-native",
  "description": "This template should help get you started developing with React Native",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "expo-router/entry",
  "author": {
    "name": "HXRRY",
    "email": "harrydng1209@gmail.com",
    "url": "https://github.com/harrydng1209"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "type-check": "tsc --build --force",
    "check-all": "run-p format lint type-check",
    "preinstall": "npx -y only-allow yarn",
    "prepare": "husky",
    "precommit": "lint-staged && validate-branch-name"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "yarn run format",
      "yarn run lint",
      "git add ."
    ]
  },
  "validate-branch-name": {
    "pattern": "^(feature|bugfix|hotfix|release)/.+|(master)$"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/elements": "^2.2.5",
    "@react-navigation/native": "^7.0.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "expo": "~52.0.20",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.2",
    "expo-haptics": "~14.0.0",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.14",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "i18next": "^24.2.1",
    "lodash-es": "^4.17.21",
    "qs": "^6.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.0",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "string-template": "^1.0.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@types/qs": "^6.9.18",
    "@types/react": "~18.3.12",
    "@types/string-template": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "validate-branch-name": "^1.3.1"
  }
}
